.editor{padding:0px; min-width:850px;}
.content{position:relative;}
.menu{width:212px; float:left; background:#4d5464; border:1px solid #ccc; border-left:0; padding-bottom:180px; position:relative; z-index:2;}
.menulist{
  height:50px; color:#fff; font-size:18px; line-height:50px; padding-left:40px; 
  padding-right:14px; border-bottom:1px solid #555c6b; background:#4d5464; cursor:pointer;
}
.menulist.active{background:#607d8b; border-bottom:1px solid #557381;}
.secend-list{
  height:40px; border-bottom:1px solid #555c6b; line-height:40px; padding-right:14px; 
  font-size:14px; color:#fff; padding-left:60px; background:#434959; cursor:pointer;
}
.menulist .iconfont,.secend-list .iconfont{font-size:16px; float:right; cursor:pointer;}
.tab{position:absolute; width:100%; left:0; top:0; padding-left:285px;}
.title{height:50px; border-bottom:1px solid #23ad44; line-height:50px; font-size:18px;}
.base-info,.info{width:100%; float:left;}
.info{margin-bottom:15px;}
.info-list{width:60%; margin-top:15px; display:inline-block; float:left; margin-right:80px;}
.info-list p,.info p{font-size:16px; line-height:30px;}
.info p span{color:#999; font-size:12px;}
.info-list input{
  width:100%; height:40px; border:1px solid #ccc; border-radius:2px; 
  padding:0px 10px; margin-top:10px; font-size:16px;
}
.info textarea{
  width:60%; height:80px; margin-top:10px; border:1px solid #ccc; border-radius:2px; 
  padding:10px; cursor:text; line-height:20px; font-size:16px;
}
.selectbox{height:30px; margin-top:10px;}
.select{
  width:200px; height:30px; line-height:30px; border:1px solid #ccc; border-radius:2px; 
  background:#fff; display:inline-block; float:left; margin-right:30px; overflow:hidden;
}
.select select{width:100%; height:28px; line-height:28px; border:0; background:none;}
.upload-box{margin-top:10px;}
.upload-box button{
  color:#fff; font-size:18px; background:#23ad44; border:0; border-radius:5px; 
  padding:0px 45px; height:40px; display:inline-block; float:left; margin-right:30px;
}
.imglist{margin-top:20px;}
.audio-list{width:60%;}
.images{width:235px; height:169px; overflow:hidden; position:relative; float:left; margin-right:21px; margin-bottom:21px; text-align:center;}
.images img{width:100%;}
.deleteimg,.chooseimg{width:20px; height:20px; display:block; position:absolute; top:10px; right:10px; cursor:pointer;}
.deleteimg .iconfont{font-size:18px; color:#fff;}
.chooseimg .iconfont{font-size:18px; color:#23ad44;}
.audio{border:1px solid #ccc; height:45px; margin:10px 0px; background:#dce0e2; line-height:45px; font-size:16px; padding:0px 30px;}
.audio .iconfont{float:right; color:#1da5f6; cursor:pointer;}
.audio.selected{background:#b5b6b7;}
.audio.selected .iconfont{display:block;}

.recovery,.keep{padding:0px 45px; color:#fff; font-size:18px; border:0; border-radius:5px; height:40px; background:#23ad44;}

.intro-main{width:60%; height:480px; border:1px solid #ccc; background:#fff; overflow:scroll; overflow-x:hidden; padding:10px; border-radius:2px;}
.selectimg{width:100%;}
.intro-main textarea{width:100%; border:0; height:100%; padding:0;}
.info-main{width:60%;}
.introbox{width:60%; height:40px; border:1px solid #ccc; border-radius:2px;}
.viewnamebox,.introbox{margin-top:10px;}
.inputbox,.introbox,.viewfind,.viewname{ padding:0px 10px; font-size:16px; height:40px; border:1px solid #ccc; border-radius:2px;}
.inputbox{width:60%; float:left;}
.viewfind{width:35%; float:right; background:#fff;}
.viewname{width:35%; float:left; background:#fff; height:30px; font-size:13px;}
.add-info{width:60%; height:600px; border:1px solid #ccc; background:#fff; border-radius:2px;}
.button-box{height:46px; border-bottom:1px solid #ccc;}
.button-box button{
  width:82px; height:26px; display:block; float:left; text-align:center; font-size:12px; 
  background:none; border:1px solid #ccc; border-radius:2px; margin:10px 15px;
}
.add-source{position:relative; float:left;}
.add-way{position:absolute; top:45px; left:15px; z-index:2; width:100px; border:1px solid #ccc; border-bottom:0; border-top:0; display:none;}
.add-way span{display:block; width:100%; height:40px; background:#fff; border-bottom:1px solid #ccc; line-height:40px; cursor:pointer; 
  text-align:center; font-size:14px;
}
.button-box button:hover{color:#23ad44; border-color:#23ad44;}
.addinfo-main{height:554px; overflow:scroll; overflow-x:hidden; padding:10px;}
.addinfo-main .images,.intro-main .images{width:144px; height:104px; overflow:hidden; text-align:center;}
.addinfo-main textarea,.intro-main textarea{width:100%; border:0; height:200px; padding:0; margin-top:0; background:none; border:1px solid #ccc; padding:10px;}
.addlist{height:35px; border:1px solid #ccc; padding:0px 5px 0px 20px; margin-top:10px;}
.list-title{line-height:35px; width:80%; height:35px; display:block; float:left; font-size:16px;}
.remove-list{height:35px; width:35px; display:block; float:right;line-height:35px; text-align:center; cursor:pointer;}
.remove-list .iconfont{font-size:18px;}


/*data member style*/
.route-list{margin-top:10px;}
.route-list input{height:30px; display:block; float:left; border:1px solid #ccc; border-radius:2px; margin-right:15px; padding:0px 10px;}
.route-list span{width:30px; height:30px; display:block; float:left; line-height:30px; text-align:center; cursor:pointer;}
.route-list span .iconfont{color:#ccc; font-size:24px;}
.latitude,.longitude{width:100px;}
.viewname{width:200px;}
.video-list{width:60%; margin-top:20px;}


/*upload images*/
.ui-dialog{width:500px; border:1px solid #ccc; background:#fff; border-radius:5px; z-index:3;}
.ui-dialog-titlebar{height:50px; background:#23ad44; position:relative; border-top-left-radius:5px; border-top-right-radius:5px;}
.ui-dialog-title{width:80%; height:50px; line-height:50px; display:block; float:left; position:absolute; top:0; left:30px; font-size:18px; color:#fff;}
.ui-dialog-titlebar-close{width:50px; height:50px; display:block; float:right; position:absolute; right:10px; top:0; color:#fff; font-size:18px; background:none; border:0;}
.dialog{padding:0px;}
.dialog-main{height:400px; overflow-y:auto; padding:30px;}
.main-left{width:140px; height:400px; border-right:1px solid #ccc; float:left;}
.imagesfiles{height:40px; border-bottom:1px solid #ccc; cursor:pointer; line-height:40px; font-size:16px; padding:0px 20px;}
.main-right{height:400px;}
.images-detail{ padding:30px 20px;}
.images-detail .images{width:134px; height:96px; text-align:center; overflow:hidden; margin:0px 15px 15px 0px;}
.images-detail .images img{width:134px;}
.dialog-footer{height:54px; background:#f3f3f3; padding:0px 30px;}
.imgnum{width:200px; height:54px; line-height:54px; float:left;}
.dialog-button{width:200px; height:54px; line-height:54px; float:right;}
.dialog-button button{
  width:80px; height:30px; display:inline-block; float:left; margin-left:20px; 
  border:0; background:#23ad44; border-radius:2px; color:#fff; margin-top:12px; line-height:30px;
}

.audio_list{border:1px solid #ccc; height:45px; margin:10px 0px; background:#dce0e2; line-height:45px; font-size:16px; padding:0px 30px;}
.audio_list .iconfont{float:right; color:#1da5f6; display:none;}
.audio_list.selected{background:#b5b6b7;}
.audio_list.selected .iconfont{display:block;}

/*趣闻检索*/
.search-header{height:40px;}
.searchbox{width:60%; height:40px; display:block; border:1px solid #ccc; background:#fff; border-radius:2px; float:left; padding:0px 10px;}
.searchbtn{width:100px; height:40px; display:block; float:left; background:#23ad44; margin-left:10px; border:0; color:#fff; font-size:16px;}
.newsbox{height:290px; margin-top:10px;}
.iseditor .member{display:none;}

@media only screen and (max-width:1250px) {
  .add-info{width:80%;}
}
@media only screen and (max-width:1000px) {
  .add-info{width:90%;}
}
@media only screen and (max-width:951px) {
 .select{width:25%;} 
 .info-list{width:60%;}
}
@media only screen and (max-width:900px) {
  .add-info{width:100%;}
}